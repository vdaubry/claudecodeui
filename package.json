{
  "name": "@siteboon/claude-code-ui",
  "version": "1.12.0",
  "description": "A web-based UI for Claude Code CLI",
  "type": "module",
  "main": "server/index.js",
  "bin": {
    "claude-code-ui": "server/cli.js",
    "cloudcli": "server/cli.js"
  },
  "files": [
    "server/",
    "src/",
    "README.md"
  ],
  "homepage": "https://claudecodeui.siteboon.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siteboon/claudecodeui.git"
  },
  "bugs": {
    "url": "https://github.com/siteboon/claudecodeui/issues"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "vite --host",
    "create-user": "node scripts/create-user.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed"
  },
  "keywords": [
    "claude coode",
    "ai",
    "anthropic",
    "ui",
    "mobile"
  ],
  "author": "Claude Code UI Contributors",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.60",
    "@codemirror/lang-css": "^6.3.1",
